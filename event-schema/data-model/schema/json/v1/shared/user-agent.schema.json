{
  "id": "https://internal-domain/schema/shared/user-agent/v1",
  "$schema": "http://json-schema.org/draft-07/schema",
  "description": "Fields to describe a browser the `user_agent` string which typically originates from an HTTP client such as a web browser or CLI application.",
  "type": "object",
  "properties": {
    "device": {
      "description": "The device representing the user agent.",
      "type": "object",
      "default": {},
      "additionalProperties": true,
      "properties": {
        "name": {
          "description": "The name of the device.",
          "type": "string",
          "default": "",
          "examples": [""]
        }
      },
      "examples": [{
        "name": ""
      }]
    },
    "name": {
      "description": "Name of the user agent.",
      "type": "string",
      "default": "",
      "examples": [.com"]
    },
    "original": {
      "description": "Unparsed user_agent string.",
      "type": "string",
      "default": "",
      "examples": [""]
    }
  },
  "examples": [{
    "name": .com"
  }]
}
